process {
    executor = 'awsbatch'
    queueSize = 100
    maxForks = 16          
    errorStrategy = 'retry'       
    maxRetries = 20  
    maxErrors = 100
}
process.resourceLabels = ['allen-batch-pipeline': 'aind-pophys-pipeline']
process {
  withName: capsule_aind_ophys_classifier_17 { containerOptions = '--shm-size 4000' }
}